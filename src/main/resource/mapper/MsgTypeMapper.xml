<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="MsgTypeMapper">

  <resultMap type="com.gandazhi.test.pojo.MsgType" id="sceneResultMap">
    <result property="id" column="ID" jdbcType="NUMERIC" />
    <result property="msgType" column="msg_type" jdbcType="VARCHAR"/>
    <result property="desc" column="desc" jdbcType="VARCHAR"/>
    <result property="createTime" column="create_time" jdbcType="DATE"/>
    <result property="updateTime" column="update_time" jdbcType="DATE" />
    <result property="deleteFlag" column="delete_flag" jdbcType="VARCHAR"/>
  </resultMap>

  <select id="selectAll" resultMap="sceneResultMap">
    select * from msg_type
  </select>

</mapper>

